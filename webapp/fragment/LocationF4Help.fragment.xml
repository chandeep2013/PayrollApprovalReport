<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
<SelectDialog
		title="{i18n>location}"
		items="{
			path: 'valueHelp>/location',
			sorter: {
				path: 'externalCode'
			}
		}"
		search="onLocationSearch"
		confirm="onConfirmLocation"
		cancel="onConfirmLocation"
		multiSelect="true">
		<StandardListItem id="idLocationTable"
			title="{valueHelp>externalCode}"
			description="{valueHelp>externalName_defaultValue}"/>
	</SelectDialog>
</core:FragmentDefinition>


